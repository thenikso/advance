{
  "name": "@nikso/rye",
  "version": "0.0.1",
  "description": "An implementation of the Rye programming language in JavaScript",
  "main": "dist/rye.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "serve",
    "test": "node test/index.js",
    "build": "vite build",
    "prepublish": "pnpm run build"
