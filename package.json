    ".": [
      {
        "import": "./index.mjs",
        "require": "./dist/tye.cjs",
        "types": "./index.d.ts"
      },
      "./dist/tye.cjs"
    ]
  },
  "keywords": [
    "rye",
    "lang",
    "language"
  ],
  "author": "Nicola Peduzzi <thenikso@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "ohm-js": "^17.1.0",
    "serve": "^14.2.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.6"
  }
}
